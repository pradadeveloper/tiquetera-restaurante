    <!-- presentacion de planes de suscripcion -->
    <section id="planes mt-4 mb-4">
      <div class="container">
        <div class="row">
          <div class="col text-center text-uppercase">
            <p class="planes-text1">Conoce nuestros planes de</p>
            <h1><strong>Planes de suscripción:</strong></h1>
          </div>
        </div>
        <div class="row">
          <div class="col-12 col-md-4">
            <div class="card">
              <img src="/images/almuerzo_1.jpg" class="card-img-top" alt="plan1">
                <div class="card-body">
                  <h5 class="card-title">EXPLORADOR</h5>
                  <p class="card-text">Tendrás una cantidad de 5 almuerzos disponibles para tu mes</p>
                </div>
                <ul class="list-group list-group-flush">
                  <li class="list-group-item">5 ALMUERZOS</li>
                  <li class="list-group-item">5 ENVIOS GRATIS</li>
                  <li class="list-group-item">Máximo 1 ubicación por entrega</li>
                </ul>
                <div class="card-body">
                  <button type="button" class="btn btn-info">Quiero ver la carta</button>
                  <button type="button" class="btn btn-success">Suscribirme!</button>
                </div>
              </div>
            </div>

            <div class="col-12 col-md-4">
              <div class="card">
                <img src="/images/almuerzo_2.jpg" class="card-img-top" alt="plan1">
                <div class="card-body">
                  <h5 class="card-title">FOODIE</h5>
                  <p class="card-text">Tendrás una cantidad de 10 almuerzos disponibles para tu mes</p>
                </div>
                <ul class="list-group list-group-flush">
                  <li class="list-group-item">10 ALMUERZOS DISPONIBLES</li>
                  <li class="list-group-item">10 ENVIOS GRATIS</li>
                  <li class="list-group-item">Máximo 2 ubicación por entrega</li>
                </ul>
                <div class="card-body">
                  <button type="button" class="btn btn-info">Quiero ver la carta</button>
                  <button type="button" class="btn btn-success">Suscribirme!</button>
                </div>
              </div>
            </div>
            <div class="col-12 col-md-4">
              <div class="card">
                <img src="/images/almuerzo_3.jpg" class="card-img-top" alt="plan1">
                <div class="card-body">
                  <div class="row">
                    <div class="col-2 offset-7">
                      <span class="badge bg-warning text-dark">Recomendado</span>
                    </div>
                  </div>
                    <h5 class="card-title">AHORRADOR</h5>
                  <p class="card-text">Tendrás una cantidad de 20 almuerzos disponibles para tu mes</p>
                </div>
                <ul class="list-group list-group-flush">
                  <li class="list-group-item">20 ALMUERZOS DISPONIBLES</li>
                  <li class="list-group-item">20 ENVIOS GRATIS</li>
                  <li class="list-group-item">Máximo 3 ubicación por entrega</li>
                </ul>
                <div class="card-body">
                  <button type="button" class="btn btn-info">Quiero ver la carta</button>
                  <button type="button" class="btn btn-success">Suscribirme!</button> 
                </div>
              </div>
            </div>

        </div>
      </div>
    </section>
